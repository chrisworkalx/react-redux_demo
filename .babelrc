{
    "presets": ["@babel/preset-env", "@babel/preset-react", "react-app"],
    "plugins": [
      "syntax-dynamic-import",
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ],
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ]
      ]
  }